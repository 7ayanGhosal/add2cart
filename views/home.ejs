		.col-test{
			background: lightgrey;
			background-color:rgba(0,0,0,0.2);
			margin: 8px 0;
			padding-top: 15px;
			border-radius: 10px;
			text-align: center;
			border: 4px solid white;
			padding-left: 0;
			padding-right:0;
			overflow:hidden;
		}
		.col-test:hover{
			background-color:rgba(44, 130, 201, 0.6);
			box-shadow: 0 0 13px black;
		}
		.img-thumbnail{
			max-width:180px;
			max-height:180px;
			width:40vw;
			height:40vw;	
		}
		.col-test .img-thumbnail:hover{
			max-width:188px;
			max-height:188px;
			width:44vw;
			height:44vw;
		}
		
		.categories{ 
			color:white;
			text-align:center;
			font-weight:bold;
			text-shadow: 2px 2px 8px black;
		}	
		.category-link{
			color:white;
			font-weight:bold;
			text-shadow: 0 0 4px black;
			
		}
		p{
			margin-top:5px;	
		}
		.contact{
			background:#86af49;
			color:white;
			opacity:0.8;
			padding-top:15px;
			border-radius:25px 25px 0 0;
			
		}
	
		
		
	</style>
